<fieldset>
	<%= f.input :kind %>
	<%= f.input :street, :required=> true %>
	<%= f.input :city, :required=> true %>
	<%= f.input :state, :required=> true %>
	<%= f.input :zip, :required=> true %>
	<%= f.input :country, :required=> true, priority: [ "United States" ] %>
	<%= f.link_to_remove "Remove an address" %>
</fieldset>